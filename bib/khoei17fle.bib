	Doi = {10.1016/j.visres.2007.10.020},
	Issn = {0042-6989},
	Journal = {Vision research},
	Keywords = {bayesian, khoei13jpp, model, motion-clouds, motion-integration, ocular\_following\_response, ofr, perrinet12pred, response\_latency, sanz12jnp; thesis},
	Month = feb,
	Number = {4},
	Pages = {501--522},
	Pmid = {18221979},
	Priority = {0},
	Title = {Dynamics of distributed {1D} and {2D} motion representations for short-latency ocular following.},
	Url = {http://dx.doi.org/10.1016/j.visres.2007.10.020},
